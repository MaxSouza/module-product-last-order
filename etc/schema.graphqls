type Query {
    hasCustomerPurchasedProduct(
        customerId: String!
        productId: String!
    ): HasPurchasedProduct
        @resolver(
            class: "Discorgento\\ProductLastOrder\\Model\\Resolver\\HasCustomerPurchasedProduct"
        )
        @cache(cacheIdentity: "Discorgento\\ProductLastOrder\\Model\\Resolver\\Identity" cacheable: true)
}

type HasPurchasedProduct {
    hasPurchased: Boolean!
    orderLink: String!
    orderDate: String!
}
